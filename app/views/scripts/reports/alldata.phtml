                   $("#StartYear").prop("disabled",false);
                   $("#EndMonth").prop("disabled",false);
                   $("#EndYear").prop("disabled",false);
                  
               }else if(period=="annual"){
                   $("#StartMonth").prop("disabled",true);
                   $("#StartYear").prop("disabled",false);
                   $("#EndMonth").prop("disabled",true);
                   $("#EndYear").prop("disabled",false);
                   
                   
               }else{
                   $("#StartMonth").prop("disabled",false);
