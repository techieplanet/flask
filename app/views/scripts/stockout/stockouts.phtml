                    foreach ($this->fp_facs_providing_stockedout as $key=>$monthProviders){ 
                        $month .= "'" . $monthProviders['month'] . "',";
                        $monthName = $monthProviders['month'];
                        $numeratorFP = $this->fp_numerator[$monthName];
                        $denominatorFP = $this->fp_denominator[$monthName];
                        
                        $numeratorLARC = $this->larc_numerator[$monthName];
                        $denominatorLARC = $this->larc_denominator[$monthName];
                        
                        
                        $seven_days_percent = round($monthProviders['seven_days_percent'] * 100,1);
                        $implant_percent = round($this->larc_facs_providing_stockedout[$key]['implant_percent'] * 100,1);
            ?>
                    jNumData['% FP facilities stocked out of any FP commodity for 7 days']['<?php echo $monthName; ?>'] = <?php echo $numeratorFP ?>;
                    
                    jDenomData['% FP facilities stocked out of any FP commodity for 7 days']['<?php echo $monthName; ?>'] = <?php echo $denominatorFP ?>;
                    
                    jNumData['% implant facilities stocked out of implants']['<?php echo $monthName; ?>'] = <?php echo $numeratorLARC ?>;
                    
                    jDenomData['% implant facilities stocked out of implants']['<?php echo $monthName; ?>'] = <?php echo $denominatorLARC ?>;
                    
                        //set PHP values into JS objects
                        sevenObject.data.push(<?php echo $seven_days_percent; ?>);
                        implantsObject.data.push(<?php echo $implant_percent; ?>);
            <?php
